{
  "builds": [
    {
      "src": "bus_predictor/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb",
        "includeFiles": [
          "bus_predictor/templates/**",
          "requirements.txt"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bus_predictor/app.py"
    }
  ]
}
